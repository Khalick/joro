jQuery(document).ready(function() {
        // Function to create and modify the node
        function createWhatsAppNode() {
            if (jQuery('.wa').length === 0) { // Check if .wa already exists
                // Duplicate the node
                var originalNode = jQuery('li.menu-item-603:nth-child(12)');
                var clonedNode = originalNode.clone();
                
                // Modify the cloned node
                clonedNode.removeClass('tw').addClass('wa'); // Remove 'tw' class and add 'wa'
                clonedNode.find('a').attr('href', 'https://api.whatsapp.com/send?phone=254738333888'); // Change the link

                // Inject custom styles into the head
                jQuery('head').append('<style>' + 
                    '.wa .av-icon-char::before { display: none; }' + 
                    '.wa .av-icon-char { ' +
                        'background: url("https://www.nomadbeachbar.com/wp-content/uploads/2023/11/whatsapp.png") !important; ' +
                        'background-repeat: no-repeat !important; ' +
                        'background-size: contain !important; ' +
                        'filter: invert(9%) sepia(78%) saturate(3681%) hue-rotate(191deg) brightness(89%) contrast(99%); ' +
                        'width: 30px; ' +
                        'height: 30px; ' +
                        'display: block !important; ' +
                        'margin-left: -10px; ' +
                        'margin-top: 10px; ' +
                    '}' +
                    '</style>'
                );
                
                // Insert the modified cloned node back into the DOM
                clonedNode.insertAfter(originalNode);
            }
        }

        // Execute only for mobile view (max-width: 767px)
        if (jQuery(window).width() <= 767) {
            // Listen for .av-burger-menu-main opening
            jQuery('.av-burger-menu-main').on('click', function() {
                createWhatsAppNode();
            });
        }
    });
</script>



